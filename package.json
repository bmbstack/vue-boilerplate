{
  "name": "vue-boilerplate",
  "base": "activity-boilerplate@3.5.0",
  "version": "2.0.0",
  "description": "vue脚手架",
  "main": "index.js",
  "scripts": {
    "test:app": "cross-env NODE_ENV=development mocha ./test/actions/**/*.spec.js ./test/mutations/**/*.spec.js --bail --compilers js:babel-core/register -t 5000 -s 1000",
    "test:api": "cross-env NODE_ENV=production mocha ./test/api/**/*.spec.js --bail --compilers js:babel-core/register -t 5000 -s 1000",
    "test:api:watch": "cross-env NODE_ENV=production mocha ./test/api/**/*.spec.js --bail --compilers js:babel-core/register -t 5000 -s 1000 --watch -R spec",
    "lint": "eslint ./src --fix",
    "doc": "jsdoc -a all -c ./build/jsdoc.config.json ./src -R ./README.md",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.development.js --inline --hot --host 0.0.0.0",
    "pre": "cross-env NODE_ENV=preview webpack --config ./build/webpack.config.preview.js --colors --progress",
    "release": "yarn run lint && cross-env NODE_ENV=production webpack --config ./build/webpack.config.production.js --colors --progress",
    "compress": "node ./build/compress.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmbstack/vue-boilerplate.git"
  },
  "author": "nixon",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "axios": "^0.15.3",
    "babel-eslint": "^7.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-minify-constant-folding": "^0.2.0",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-minify-guarded-expressions": "^0.2.0",
    "babel-plugin-minify-simplify": "^0.2.0",
    "babel-plugin-minify-type-constructors": "^0.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-consecutive-adds": "^0.2.0",
    "babel-plugin-transform-member-expression-literals": "^6.8.5",
    "babel-plugin-transform-merge-sibling-variables": "^6.8.6",
    "babel-plugin-transform-minify-booleans": "^6.8.3",
    "babel-plugin-transform-node-env-inline": "^0.2.0",
    "babel-plugin-transform-property-literals": "^6.8.5",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-plugin-transform-remove-debugger": "^6.8.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.12.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "mocha": "^3.2.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.5.2",
    "normalize.css": "^4.2.0",
    "postcss-loader": "^1.2.0",
    "precss": "^1.4.0",
    "rxjs": "^5.3.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0",
    "tinify": "^1.5.0",
    "vue-class-component": "^5.0.2",
    "vue-router": "^2.7.0",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^0.14.1",
    "webpack-shell-plugin": "^0.5.0"
  }
}
