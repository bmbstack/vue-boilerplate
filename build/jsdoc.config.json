{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_|node_modules|src/static"
    },
    "plugins": ["../node_modules/jsdoc-babel"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./doc/",
        "recurse": true
    }
}
